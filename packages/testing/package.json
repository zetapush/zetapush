{
  "name": "@zetapush/testing",
  "version": "0.37.5",
  "description": "Zeta Push Next",
  "browser": "dist/zetapush-testing.js",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "lib",
    "es",
    "index.d.ts",
    "LICENSE"
  ],
  "repository": "https://github.com/zetapush/zetapush",
  "author": "ci@zetapush.com",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf lib dist es .deploy .esdoc",
    "build:commonjs": "tsc --outDir lib",
    "build:es": "tsc --module ES2015 --moduleResolution Node --outDir es",
    "build:umd": "cross-env NODE_ENV=development rollup -c -i src/index.ts -o dist/zetapush-testing.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -i src/index.ts -o dist/zetapush-testing.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "prepare": "npm run clean && npm run build"
  },
  "dependencies": {
    "@zetapush/client": "0.37.5",
    "@zetapush/cometd": "0.37.5",
    "@zetapush/common": "0.37.5",
    "@zetapush/core": "0.37.5",
    "@zetapush/worker": "0.37.5",
    "chalk": "2.4.1",
    "copy-dir": "0.4.0",
    "execa": "1.0.0",
    "find-free-port": "2.0.0",
    "rimraf": "2.6.2",
    "tree-kill": "1.2.1",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "@types/execa": "0.9.0",
    "@types/jasmine": "3.3.0",
    "@types/node": "10.12.9",
    "@types/rimraf": "2.0.2",
    "cross-env": "5.2.0",
    "execa": "1.0.0",
    "find-free-port": "2.0.0",
    "jasmine": "3.3.0",
    "jasmine-reporters": "2.3.2",
    "js-yaml": "3.12.0",
    "localtunnel": "1.9.1",
    "rollup": "0.67.3",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-json": "3.1.0",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-terser": "3.0.0",
    "rollup-plugin-typescript": "1.0.0",
    "rollup-watch": "4.3.1",
    "typescript": "3.1.6",
    "verdaccio": "3.8.6"
  },
  "publishConfig": {
    "access": "public"
  }
